{
  "name": "soma",
  "version": "1.0.0",
  "description": "SOMA - Self-Organizing Metacognitive Architecture",
  "main": "dist-electron/main.js",
  "type": "module",
  "bin": {
    "soma-cli": "./cli/soma-cli.mjs"
  },
  "scripts": {
    "dev": "vite",
    "dev:backend": "node launcher_ULTRA.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node launcher_ULTRA.mjs",
    "backend": "node launcher_ULTRA.mjs",
    "electron:dev": "echo âŒ DEPRECATED - Use 'npm run start:all' instead! It now launches Electron in dev mode with hot-reload.",
    "electron:build": "vite build && electron-builder",
    "electron:pack": "electron-builder --dir",
    "unified:test": "node arbiters/UnifiedMemoryArbiter-INTEGRATED.js",
    "soma": "node launcher_ULTRA.mjs",
    "soma:test": "node tests/quick_test.mjs",
    "soma:cluster:coordinator": "set SOMA_CLUSTER=true&& set SOMA_ROLE=coordinator&& node launcher_ULTRA.mjs",
    "soma:cluster:worker": "set SOMA_CLUSTER=true&& set SOMA_ROLE=worker&& node launcher_ULTRA.mjs",
    "start:all": "node start-dev.cjs",
    "preflight": "node core/PreFlightCheck.js",
    "safe-start": "node core/PreFlightCheck.js && node start-dev.cjs",
    "health": "node health-check.cjs",
    "kill": "taskkill /F /IM electron.exe /T 2>nul & powershell -NoProfile -Command \"Get-NetTCPConnection -LocalPort 3001,8081 -ErrorAction SilentlyContinue | Select-Object -ExpandProperty OwningProcess | Sort-Object -Unique | ForEach-Object { taskkill /F /PID $_ 2>nul }\"",
    "restart": "npm run kill && timeout /t 2 /nobreak && npm run start:all",
    "cli": "node cli/soma-cli.mjs"
  },
  "build": {
    "appId": "com.soma.dashboard",
    "productName": "SOMA",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.png"
    }
  },
